{
  "name": "snowflake-pool",
  "version": "1.0.0",
  "description": "Snowflake connection pool",
  "main": "./dist/index.js",
  "repository": "",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
