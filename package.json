  },
  "devDependencies": {
    "@types/generic-pool": "^3.1.9",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "typescript": "4.2.4"
  }
